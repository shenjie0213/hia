<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
	<package name="urole" namespace="/" extends="base">
		<!-- 产品分类Action-->
         <action name="urole_*" class="uRoleAction" method="{1}">
        	<result name="list">/WEB-INF/jsp/uroleAction/list.jsp</result>
        	<result name="saveUI">/WEB-INF/jsp/uroleAction/saveUI.jsp</result>
        	<result name="setPrivilegeUI">/WEB-INF/jsp/uroleAction/setPrivilegeUI.jsp</result>
        	<result name="tolist" type="redirectAction">urole_list</result>
        </action>
        </package> 
       <!--  分类属性Action -->
	

</struts>
