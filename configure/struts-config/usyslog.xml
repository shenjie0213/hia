<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-config/gattribute.xml"></include>
	
	<package name="usyslog" namespace="/" extends="base">
		<!-- 产品分类Action-->
         <action name="usyslog_*" class="cn.ssh.action.USyslogAction" method="{1}">
        	<result name="list">/WEB-INF/jsp/usyslogAction/list.jsp</result>
        	<result name="tolist" type="redirectAction">usyslog_list</result>
        </action>
        </package> 
       <!--  分类属性Action -->
	

</struts>
