<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
	<package name="uevent" namespace="/" extends="base">
        <action name="uevent_*" class="uEventAction" method="{1}">
        	<result name="list">/WEB-INF/jsp/ueventAction/list.jsp</result>
        	<result name="saveUI">/WEB-INF/jsp/ueventAction/saveUI.jsp</result>
        	<result name="tolist" type="redirectAction">uevent_list</result>
        </action>
	</package>

</struts>
